{
  "name": "@scribe.js/tesseract.js",
  "version": "7.0.1",
  "description": "Fork of tesseract.js used for scribe.js. Pure Javascript Multilingual OCR",
  "type": "module",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "unpkg": "dist/tesseract.min.js",
  "jsdelivr": "dist/tesseract.min.js",
  "scripts": {
    "test": "npm-run-all -p test:all",
    "test:all": "npm-run-all test:browser test:node:all",
    "test:browser": "karma start karma.conf.cjs",
    "test:node": "mocha --exit --bail ./tests/*.test.js",
    "test:node:all": "npm run test:node",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src"
  },
  "browser": {
    "./src/worker/node/index.js": "./src/worker/browser/index.js"
  },
  "author": "Balearica",
  "license": "AGPL-3.0",
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.1",
    "chai": "^5.1.2",
    "karma": "^6.4.2",
    "karma-chrome-launcher": "^3.2.0",
    "karma-firefox-launcher": "^2.1.2",
    "karma-mocha": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "mocha": "^11.7.5",
    "npm-run-all": "^4.1.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/scribeocr/tesseract.js.git"
  },
  "bugs": {
    "url": "https://github.com/scribeocr/tesseract.js/issues"
